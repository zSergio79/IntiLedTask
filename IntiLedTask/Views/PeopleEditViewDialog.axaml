<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="using:IntiLedTask.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="IntiLedTask.PeopleEditViewDialog"
        Title="PeopleEditDialog" WindowStartupLocation="CenterOwner"
        Width="800" Height="300">
  <Window.Styles>
    
  </Window.Styles>
  <Border Margin="10" Padding="5" BorderBrush="Gray" BorderThickness="1">
    <Grid RowDefinitions="*, Auto">
      <views:PeopleEditView Grid.Row="0"/>
      <StackPanel Grid.Row="1" Orientation="Horizontal" Margin="0,10" HorizontalAlignment="Center">
        <Button Content="Ok" IsDefault="True" Click="ButtonOk"/>
        <Button Content="Cancel" IsCancel="True" Click="ButtonCancel"/>
      </StackPanel>
    </Grid>
  </Border>
</Window>
